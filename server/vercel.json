{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**"],
        "maxLambdaSize": "50mb",
        "memory": 3008,
        "suppressResponse": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}